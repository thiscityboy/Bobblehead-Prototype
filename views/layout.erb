<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title> </title>
  <script type="text/javascript">
	  	// Device detect
	  	var deviceAgent = navigator.userAgent.toLowerCase();
	  	var agentID = deviceAgent.match(/(android 3.0|android 3.2)/);
	     if (agentID) { //ios
		     	// _gaq.push(['_trackEvent', 'Detect', 'NHL', 'App']);
		     	setTimeout(function(){
		     		document.location='/incompatible';
		     	},500);			      
	    	}     
	</script>

  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <meta name="apple-mobile-web-app-capable" content="yes">

  <link href="http://dqo839kq1ibvx.cloudfront.net/bootstrap_v2_1_0/css/bootstrap_no_icons.min.css" rel="stylesheet">
  <link href="http://dqo839kq1ibvx.cloudfront.net/bootstrap_v2_1_0/css/bootstrap-responsive.min.css" rel="stylesheet">

  <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
  <!--[if lt IE 9]>
  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->

  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
  <% if @js %>
      <% @js.each do |script| %>
          <%= "<script src='#{script}' type='text/javascript'></script>" %>
      <% end %>
  <% end %>
  <% if @css %>
      <% @css.each do |style| %>
          <%= "<link rel='stylesheet' href='#{style}' type='text/css' />"  %>
      <% end %>
  <% end %>
  <link href="/css/common.css" rel="stylesheet">
</head>

<body>
	
	<!--start: Header -->
	<header>
		
		<!--start: Container -->
		<div class="container">
			
			<!--start: Row -->
			<div class="row">
					
				<!--start: Logo -->
				<div class="span3">
						
				<a style="padding-top:10px" ref="#"></a>
						
				</div>
				<!--end: Logo -->
					
				<!--start: Navigation -->
				<div class="span9">
					
					<div class="navbar navbar-inverse">
			    		<div class="navbar-inner">
			          		<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
			            		<span class="icon-bar"></span>
			            		<span class="icon-bar"></span>
			            		<span class="icon-bar"></span>
			          		</a>
			          		<div class="nav-collapse collapse">
			            		<ul class="nav">
									<li><a href="#">Item 1</a></li>
							          <li><a href="#">Item 2</a></li>
									
			            		</ul>
			          		</div>
			        	</div>
			      	</div>
					
				</div>	
				<!--end: Navigation -->
					
			</div>
			<!--end: Row -->
			
		</div>
		<!--end: Container-->			
			
	</header>
	<!--end: Header-->	
	

<div id="content">
  <div class="container-fluid">
    <%= styled_flash %>
    <%= yield %>
  </div> <!-- /container -->
</div>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#flash').delay(5000).fadeOut(400);
        });
    </script>
</body>
</html>
